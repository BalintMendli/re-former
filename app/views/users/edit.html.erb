<%= form_for @user do |f| %>
  <%= render "error_messages" %>
  <%= f.label :username, 'Your username here: ' %>
  <%= f.text_field :username, value: 'username' %><br />
  <%= f.label :email, 'Email: ' %>
  <%= f.email_field :email, placeholder: 'example@example.com' %><br />
  <%= f.label :password, 'Password: ' %>
  <%= f.password_field :password %><br />
  <%= f.submit %>
<% end %>